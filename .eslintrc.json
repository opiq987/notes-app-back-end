{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-trailing-spaces": "off",
        "no-multiple-empty-lines": "off",
        "linebreak-style":"off",
        "indent":"off",
        "eol-last":"off",
        "no-unused-vars":"off"
        
    }
}
